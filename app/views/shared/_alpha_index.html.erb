<span class="pagination">
  <% ("A".."Z").each do |l| -%>	    
     <%= @alpha_index == l ? "<span class='current'>#{l}</span>" : link_to(l, :browse => 'alpha', :alpha_index => l) %>	
  <% end %>
</span>
<br/><span class="pagination" style="padding-left:3px;"><%= will_paginate(@results, :container => false) %></span>